{
    "name": "tsantos/symfony-generated-normalizer",
    "description": "Generate dedicated normalizers for your objects to increase serialization performance",
    "type": "library",
    "require": {
        "php": ">8.1",
        "symfony/serializer": "^6.1",
        "symfony/property-info": "^6.1",
        "phpdocumentor/reflection-docblock": "^5.3",
        "nette/php-generator": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpbench/phpbench": "^1.2",
        "symfony/property-access": "^6.1",
        "symfony/cache": "^6.1",
        "friendsofphp/php-cs-fixer": "^3.10"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Tsantos\\Symfony\\Serializer\\Normalizer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tsantos\\Test\\Symfony\\Serializer\\Normalizer\\": "test/"
        }
    },
    "authors": [
        {
            "name": "tsantos",
            "email": "tales.augusto.santos@gmail.com"
        }
    ],
    "config": {
        "bin-dir": "./bin"
    }
}
